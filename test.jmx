def producer = producers[randomIndex]

// 2. Генерируем XML-сообщение
def xmlMessage = &quot;&quot;&quot;
		&lt;transaction&gt;
		    &lt;id&gt;${UUID.randomUUID()}&lt;/id&gt;
		    &lt;currency&gt;USD&lt;/currency&gt;
		&lt;/transaction&gt;
&quot;&quot;&quot;.trim().replaceAll(&quot;\t&quot;, &quot;&quot;)

// 3. Создаем запись Ключ например txn_1752705499261
def record = new ProducerRecord&lt;String, String&gt;(&quot;${TopicNameProduce}&quot;, 
										&quot;txn_&quot; + System.currentTimeMillis(), 
										xmlMessage)

